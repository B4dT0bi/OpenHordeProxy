
# Your AI Horde API key used for authenticating requests to AI Horde
AI_HORDE_API_KEY=your_ai_horde_api_key

# Server host and port configuration
HOST=0.0.0.0
PORT=5000

# Default AI Horde model to use if no specific model is mapped
DEFAULT_HORDE_MODEL=stable_diffusion

# Path to save generated images
IMAGE_SAVE_PATH=requested_images

# Base URL used for generating the image URL in the API response
SERVER_BASE_URL=http://localhost:5000/images/

# Configuration for LoRAs (Low-Rank Adaptations)
# Specify multiple LoRAs using a comma-separated list (format: name:model:clip:inject_trigger)
LORAS=GlowingRunesAI,AnotherLora:2

# Model mapping from OpenAI models to AI Horde models
OPENAI_MODEL_MAP_dall_e_2=stable_diffusion
OPENAI_MODEL_MAP_dall_e_3='SDXL 1.0'

# Additional generation parameters (optional)
# Specify the sampler to use (e.g., k_euler, ddim, etc.)
SAMPLER_NAME=k_euler

# CFG scale: How closely the generated image should match the prompt (higher = more fidelity)
CFG_SCALE=7

# Clip skip: How many layers to skip when processing the input (affects speed/quality trade-off)
CLIP_SKIP=1

# Steps: Number of inference steps for generating images (higher = more detailed images)
STEPS=30

# High-resolution fix: Enable or disable upscaling for more detailed output (True/False)
HIRES_FIX=False
